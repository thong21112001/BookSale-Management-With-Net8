@if (TempData["Type"] != null)
{
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>
    <script type="text/javascript">
        var typeData = '@TempData["Type"]'
        toastr.options = {
            "closeButton": true,
            "debug": false,
            "newestOnTop": false,
            "progressBar": true,
            "positionClass": "toast-top-right",
            "preventDuplicates": false,
            "onclick": null,
            "showDuration": "300",
            "hideDuration": "1000",
            "timeOut": "5000",
            "extendedTimeOut": "1000",
            "showEasing": "swing",
            "hideEasing": "linear",
            "showMethod": "fadeIn",
            "hideMethod": "fadeOut"
        };
        if (typeData === "warning") {
            toastr["warning"]('@TempData["Message"]');
        }
        else if (typeData === "error") {
            toastr["error"]('@TempData["Message"]');
        }
        else if (typeData === "success") {
            toastr["success"]('@TempData["Message"]');
        } 
        else {
            toastr["info"]('@TempData["Message"]');
        }
    </script>
}